{
  "extends": [
    "airbnb"
  ],
  "parserOptions": {
    "ecmaVersion": 2018,
    "parser": "@babel/eslint-parser",
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "env": {
    "browser": true,
    "node": true,
    "es6": true,
    "jest": true
  },
  "rules": {
    "no-debugger": "off",
    "no-unused-vars": "warn",
    "react/prop-types": "warn",
    "jsx-a11y/control-has-associated-label": "off"
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  },
  "root": true
}